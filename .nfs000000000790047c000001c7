#!/bin/bash

cd bin





#cp data_10_copy/* data/
#cd data
#echo 1 > numFWsample.dat
#cd ..
#./gep 1 0

bendersFlag=1
imprvdBendersFlag=0

for T in $(seq 7 10)
do
	echo "Time horizon: $T; Benders' cut: $bendersFlag; Improved Benders' cut: $imprvdBendersFlag" >> table.txt
	cp data_$T/* data/
	for n in 1 2 3 5 10 20 50
	do
		cd data
		echo $n > numFWsample.dat
		cd ..
		./gep $bendersFlag $imprvdBendersFlag
	done
done

imprvdBendersFlag=1

for T in $(seq 3 10)
do
	echo "Time horizon: $T; Benders' cut: $bendersFlag; Improved Benders' cut: $imprvdBendersFlag" >> table.txt
	cp data_$T/* data/
	for n in 1 2 3 5 10 20 50
	do
		cd data
		echo $n > numFWsample.dat
		cd ..
		./gep $bendersFlag $imprvdBendersFlag
	done
done

